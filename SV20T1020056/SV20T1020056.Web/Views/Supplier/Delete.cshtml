@model SV20T1020056.DomainModels.Supplier
@*

*@
<div class="box box-primary">
    <div class="box-body">
        <form action="~/Supplier/Delete/@Model.SupplierID" method="post">
            <div class="form-group">
                <label>Tên nhà cung cấp:</label>
                <p class="form-control-static" name="SupplierName">@Model.SupplierName</p>
            </div>
            <div class="form-group">
                <label>Tên giao dịch:</label>
                <p class="form-control-static" name="ContactName">@Model.ContactName</p>
            </div>
            <div class="form-group">
                <label>Số điện thoại:</label>
                <p class="form-control-static" name="Phone">@Model.Phone</p>
            </div>
            <div class="form-group">
                <label>Email:</label>
                <p class="form-control-static" name="Email">@Model.Email</p>
            </div>
            <div class="form-group">
                <label>Địa chỉ:</label>
                <p class="form-control-static" name="Address">@Model.Address</p>
            </div>
            <div class="form-group">
                <label>Tỉnh/thành:</label>
                <p class="form-control-static" name="Provice">@Model.Provice</p>
            </div>

            <div class="from-group text-right">
                    <button type="submit" class="btn btn-danger">
                        <i class="fa fa-floppy"> </i>
                        Xóa dữ liệu
                    </button>
                    <a class="btn btn-default" href="~/Supplier">Quay lại</a>
            </div>
        </form>
    </div>
</div>


