<div class="box box-primary">
    <div class="box-body">
        <form action="#">
            <div class="form-group">
                <label>Tên khách hàng:</label>
                <input type="text" class="form-control" id="#" placeholder="Nhập tên khách hàng">
            </div>
            <div class="form-group">
                <label>Tên giao dịch:</label>
                <input type="text" class="form-control" id="#" placeholder="Nhập tên giao dịch">
            </div>
            <div class="form-group">
                <label>Số điện thoại:</label>
                <input type="text" class="form-control" id="#" placeholder="Nhập số điện thoại">
            </div>
            <div class="form-group">
                <label>Email:</label>
                <input type="email" class="form-control" id="#" placeholder="Nhập email">
            </div>
            <div class="form-group">
                <label>Địa chỉ:</label>
                <input type="text" class="form-control" id="#" placeholder="Nhập địa chỉ">
            </div>
            <div class="form-group">
                <label>Tỉnh thành:</label>
                <select class="form-control" name="Provice">
                    <option>--Chọn tỉnh thành--</option>
                    <option>Huế</option>
                    <option>Quản Trị</option>
                </select>
            </div>
            <div class="form-group">
                <label>Trạng thái:</label>
                <select class="form-control" name="Islockes">
                    <option>Tài khoản bị khóa</option>
                    <option>Tài khoản không bị khóa</option>
                </select>
                <div class="checkbox">
                    <label>
                        <input type="checkbox" name="IsLocked" value="true" />Tài khoản bị khóa
                    </label>
                    <label>
                        <input type="checkbox" name="IsLocked" value="true" /> Tài khoản không bị khóa
                    </label>
                </div>
            </div>
            <div class="from-group text-right">
                <button type="submit" class="btn btn-primary">
                    <i class="fa fa-floppy"> </i>
                    Lưu dữ liệu
                </button>
                <a href="~/Customer" class="btn btn-default">Quay lại</a>
            </div>
        </form>
    </div>
</div>


