@model SV20T1020056.DomainModels.Shipper
@{

}
<div class="box box-primary">
    <div class="box-body">
        <form action="~/Shipper/Delete/@Model.ShipperID" method="post">
            <div class="form-group">
                <label>Tên người giao hàng:</label>
                <p class="form-control-static">@Model.ShipperName</p>
            </div>
            <div class="form-group">
                <label>Số điện thoại:</label>
                <p class="form-control-static">@Model.ShipperID</p>
            </div>
            <div class="from-group text-right">
                <button type="submit" class="btn btn-danger">
                    <i class="fa fa-floppy"> </i>
                    Xóa dữ liệu
                </button>
                <a class="btn btn-default" href="~/Shipper/Index">Quay lại</a>
            </div>
        </form>
    </div>
</div>

