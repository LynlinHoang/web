@model UIWeb.Models.DTO.AddSupplierProvinceViewModel
<div class="box box-primary">

    <div class="box-header with-border ">
        <h3 class="box-title">Thông tin nhà cung cấp</h3>
        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse">
                <i class="fa fa-minus"></i>
            </button>
        </div>
    </div>

    <div class="box-body">
        <form class="form-horizontal" method="post" enctype="multipart/form-data">

            <div class="form-group">
                <label class="control-label col-sm-2">Tên loại hàng:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Nhập tên nhà cung cấp" autofocus asp-for="@Model.AddSupplier.SupplierName">
                </div>

            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Tên giao dịch:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Nhập tên giao dịch" asp-for="@Model.AddSupplier.ContactName"></textarea>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Số điện thoại:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Nhập số điện thoại" asp-for="@Model.AddSupplier.Phone"></input>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Gmail:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Nhập địa chỉ Email" asp-for="@Model.AddSupplier.Email"></input>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Địa Chỉ:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Nhập địa chỉ" asp-for="@Model.AddSupplier.Address"></input>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Tỉnh/Thành:</label>
                <div class="col-sm-10">
                    <select class="form-control" >
                        <option value="0">--Tỉnh thành--</option>
                    @*@foreach (var item in Model.provinceDtos)
                        {
                            <option value="@item.ProvinceName">@item.ProvinceName</option>
                        } *@
                    </select>

                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-primary">
                        <i class="fa fa-save"></i> Lưu dữ liệu
                    </button>
                    <a class="btn btn-warning" href="~/Categorie">
                        Quay lại
                    </a>
                </div>
            </div>
        </form>
    </div>
</div>


